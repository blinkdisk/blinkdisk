{
  "name": "@blinkdisk/cloud",
  "type": "module",
  "scripts": {
    "dev": "cp ../../.env .dev.vars && wrangler dev --port=4001 --inspector-port=4002 --persist-to=../../.wrangler",
    "deploy": "tsc --noEmit && wrangler deploy --minify",
    "typecheck": "tsc --noEmit",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@aws-sdk/s3-request-presigner": "^3.886.0"
  },
  "devDependencies": {
    "@blinkdisk/eslint": "workspace:*",
    "@blinkdisk/typescript": "workspace:*",
    "wrangler": "^4.32.0"
  }
}
